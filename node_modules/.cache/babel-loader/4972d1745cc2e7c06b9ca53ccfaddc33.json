AEE,EAFF,UAEE,EADF,YAAA,CAA2E,QAA3E,EAA2E;AAAlE,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,OAAA,IAAA,QAAA,CAAA,OAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAA6D,GAA3E,E,iBAA2B,KAAA,CAAA,OAAA,CAAQ,QAAR,CAAiB,MAAjB,CAAwB,a,CAAnD,EAAgE;AAAA;AAAhE,GACE,CAJJ,CAKI,CANN,C,IAOgB,KAAA,CAAA,K,kBAAhB,YAAA,CAEM,KAFN,EAEM,UAFN,EAEM,CADJ,YAAA,CAAwD,GAAxD,EAAwD,IAAxD,EAAwD,gBAAA,CAAnD,KAAA,CAAA,OAAA,CAAQ,QAAR,CAAiB,MAAjB,CAAwB,WAA2B,CAAA,GAAhB,gBAAA,CAAI,KAAA,CAAA,MAAJ,CAAxC,EAAkD;AAAA;AAAlD,GACI,CAFN,C,qCAGI,CAXN,C","sourcesContent":["<template>\n  <div class=\"resize__main-panel\">\n    <div v-if=\"!ready\">\n      <p>\n        {{strings.gameView.resize.disclaimer}}\n        <br>\n        <button @click=\"readyUp\">{{strings.gameView.resize.readyUpButton}}</button>\n      </p>\n    </div>\n    <div v-else-if=\"ready\">\n      <p>{{strings.gameView.resize.playerReady}}{{points}}</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport websocketEvents from \"../../constants/websocketEvents\";\nimport {strings} from \"../../constants/constants\";\n\nexport default {\n  name: \"ResizeComponent\",\n  props: {\n    socket: Object\n  },\n  data(){\n    return {\n      strings: strings,\n      ready: false,\n      width: 0,\n      height: 0,\n      points: \"\"\n    }\n  },\n  computed: {\n    checkDimensions: function() {\n      return this.width > this.height;\n    }\n  },\n  methods: {\n    readyUp(){\n      this.socket.emit(websocketEvents.READY_UP);\n      this.ready = true;\n    }\n  },\n  mounted() {\n\n    setInterval(()=>{\n      if(this.points.length >= 3) {\n        this.points = \"\";\n      } else {\n        this.points += \".\";\n      }\n    }, 1000);\n\n    this.width = window.innerWidth;\n    this.height = window.innerHeight;\n\n    if(this.checkDimensions) this.readyUp();\n\n    window.addEventListener(\"resize\", () => {\n      this.width = window.innerWidth;\n      this.height = window.innerHeight;\n      if(this.checkDimensions) this.readyUp();\n    });\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n.resize__main-panel{\n  width: 100%;\n  height: 100%;\n\n  div{\n    height: 100%;\n    display: flex;\n    flex-flow: column;\n    align-items: center;\n    justify-content: space-evenly;\n    padding: 0 5% 0 5%;\n\n    p{\n      margin: 0;\n      color: white;\n      font-size: 150%;\n    }\n\n    button{\n      font-size: 100%;\n      margin: 20px;\n    }\n  }\n}\n</style>"],"sourceRoot":""}]}