        o.setStatic(true);\n        o.setOnCollide(collision => {\n            const body = getBodyFromCollision(o.body.id, collision);\n            if(body.gameObject) {\n                if(obj.killable && body.collisionFilter.category === ctx.bulletsCategory){\n                    o.destroy();\n                }\n            }\n        });\n    });\n};"]}]}