GAACG,CAA9B,EAAiCF,CAAjC,EAAoCD,SAApC,EAA+CA,SAA/C;AACAE,UAAAA,YAAY,GAAGA,YAAY,KAAK,GAAjB,GAAuB,QAAvB,GAAkC,GAAjD;AACH;;AAEDD,QAAAA,CAAC,IAAID,SAAL;AACH;AACJ;;;WAED,uBAAa;AAAA;;AACT,WAAK7B,OAAL,CAAaiB,OAAb,CAAqB,UAACvB,MAAD,EAASyC,KAAT,EAAmB;AACpCzC,QAAAA,MAAM,CAAC0B,IAAP,GAAc,MAAI,CAACgB,OAAL,CAAaT,GAAb,CAAiBU,MAAjB,CAAwB,MAAI,CAACzC,SAAL,IAAkBF,MAAM,CAAC4C,IAAP,GAAY,GAA9B,CAAxB,EAA4D,MAAI,CAACxC,UAAL,IAAmBqC,KAAK,GAAC,GAAzB,CAA5D,EAA2F,SAAOzC,MAAM,CAAC6C,KAAzG,CAAd;AACA,YAAMC,SAAS,GAAGC,IAAI,CAACC,GAAL,CAAS,MAAI,CAAC9C,SAAd,EAAyB,MAAI,CAACE,UAA9B,IAA4C,GAA5C,GAAkD2C,IAAI,CAACE,GAAL,CAASjD,MAAM,CAAC0B,IAAP,CAAYvB,KAArB,EAA4BH,MAAM,CAAC0B,IAAP,CAAYrB,MAAxC,CAApE;AACAL,QAAAA,MAAM,CAAC0B,IAAP,CAAYwB,QAAZ,CAAqBJ,SAArB;AACH,OAJD;AAKH;;;WAED,6BAAmB;AAAA;;AACf,WAAKxC,OAAL,CAAaiB,OAAb,CAAqB,UAAAvB,MAAM,EAAI;AAC3BA,QAAAA,MAAM,CAAC0B,IAAP,CAAYI,YAAZ,CAAyBiB,IAAI,CAACI,IAAL,CAAUnD,MAAM,CAACyB,EAAP,GAAUzB,MAAM,CAAC4C,IAA3B,IAAmC,MAAI,CAAC/C,KAAjE;;AACA,YAAGG,MAAM,CAACyB,EAAP,GAAYzB,MAAM,CAAC4C,IAAtB,EAA2B;AACvB5C,UAAAA,MAAM,CAAC0B,IAAP,CAAYK,kBAAZ,CAA+B,MAAI,CAACjC,YAApC;AACH;AACJ,OALD;AAMH;;;;EA1FqCV,MAAM,CAACgE,K;;SAA5B5D,Y","sourcesContent":["import Phaser from \"phaser\";\nimport {gameDimensions, sceneKeys} from \"../constants/gameSettings\";\nimport websocketEvents from \"../constants/websocketEvents\";\n\nexport default class RankingScene extends Phaser.Scene {\n\n    constructor(socket, pointsToWin) {\n        super({key: sceneKeys.ranking});\n\n        this.socket = socket;\n        this.pointsToWin = pointsToWin;\n\n        this.speed = 300;\n        this.angularSpeed = 1200;\n    }\n\n    init(data){\n        this.player = data.player;\n        this.timer = data.timer;\n        this.bandWidth = gameDimensions.width / (this.pointsToWin+1);\n        this.lineHeight = gameDimensions.height / this.players.length;\n    }\n\n    preload(){\n        this.load.image(\"ship0\", \"./ships/ship0.png\");\n        this.load.image(\"ship1\", \"./ships/ship1.png\");\n        this.load.image(\"ship2\", \"./ships/ship2.png\");\n        this.load.image(\"ship3\", \"./ships/ship3.png\");\n    }\n\n    create(){\n        this.socket.on(websocketEvents.GAME_MODIFIED, game => {\n            this.scene.start(sceneKeys.game, game);\n        });\n\n        this.drawFinishLine();\n\n        this.createShips();\n\n        this.setShipsMovements();\n        this.scene.pause();\n        setTimeout(()=>{\n            this.scene.resume();\n        }, 1000);\n\n    }\n\n    update(){\n        if(Date.now()>=this.timer) this.scene.start(sceneKeys.game);\n        this.players.forEach(player => {\n            const target = this.bandWidth * (player.to+0.5);\n            if(\n                (player.ship.x >= target && player.ship.body.velocity.x > 0) ||\n                (player.ship.x <= target && player.ship.body.velocity.x < 0)\n            ) {\n                player.ship.setVelocityX(0);\n                player.ship.setAngularVelocity(0);\n            }\n        });\n    }\n\n    drawFinishLine(){\n        const graphics = this.add.graphics();\n        const numberOfSquares = 3;\n        const squareDim = this.bandWidth / numberOfSquares;\n        let x = this.bandWidth * this.pointsToWin;\n        let y = 0;\n        let currentColor = 0x0\n        while( y < gameDimensions.height){\n            for(let i = 0; i < numberOfSquares; i++){\n                graphics.fillStyle(currentColor, 1.0);\n                graphics.fillRect(x+squareDim*i, y, squareDim, squareDim);\n                currentColor = currentColor === 0x0 ? 0xCCCCCC : 0x0;\n            }\n\n            y += squareDim;\n        }\n    }\n\n    createShips(){\n        this.players.forEach((player, index) => {\n            player.ship = this.physics.add.sprite(this.bandWidth * (player.from+0.5), this.lineHeight * (index+0.5), \"ship\"+player.color);\n            const shipScale = Math.min(this.bandWidth, this.lineHeight) * 0.5 / Math.max(player.ship.width, player.ship.height);\n            player.ship.setScale(shipScale);\n        });\n    }\n\n    setShipsMovements(){\n        this.players.forEach(player => {\n            player.ship.setVelocityX(Math.sign(player.to-player.from) * this.speed);\n            if(player.to < player.from){\n                player.ship.setAngularVelocity(this.angularSpeed);\n            }\n        });\n    }\n}"]}]}