AEJ,EAAA,KAAK,EAAC;;;;;;uBAFb,YAAA,CASM,KATN,EAAA,UAAA,EASM,CARJ,YAAA,CAA6C,GAA7C,EAA6C,IAA7C,EAA6C,gBAAA,CAAxC,KAAA,CAAA,OAAA,CAAQ,QAAR,CAAiB,KAAjB,CAAuB,WAAiB,CAA7C,EAAuC,CAAvC,CAQI,EAPJ,YAAA,CAMM,KANN,EAAA,UAAA,EAMM,E,kBALJ,YAAA,CAIyD,SAJzD,EAIyD,IAJzD,EAIyD,WAAA,CAJpC,QAAA,CAAA,eAIoC,EAJrB,UAAxB,KAAwB,EAAnB;yBAAjB,YAAA,CAIyD,KAJzD,EAIyD;AAHnD,MAAA,GAAG,EAAE,KAG8C;AAFpD,MAAA,KAAK,EAAC,iBAE8C;AADnD,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,WAAA,CAAY,KAAZ,CAAF;AAAA,OAC8C;AAAnD,MAAA,GAAG,EAAE,OAAO,CAAA,wBAAuB,KAAvB,GAA4B,MAA5B;AAAuC,KAJzD,E,IAAA,E,CAAA,E,kBAAA,C;GAIyD,CAJzD,E,GAAA,CAKI,EANN,CAOI,CATN,C","sourcesContent":["<template>\n  <div class=\"change-color\">\n    <p>{{strings.gameView.lobby.changeColor}}</p>\n    <div class=\"colors-wrapper\">\n      <img v-for=\"color in availableColors\"\n           :key=\"color\"\n           class=\"color-available\"\n           @click=\"changeColor(color)\"\n           :src=\"require('@/assets/ships/ship'+color+'.png')\"/>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {strings, colors} from \"../../../constants/constants\";\nimport websocketEvents from \"../../../constants/websocketEvents\";\n\nexport default {\n  name: \"ChangeColor\",\n  props: {\n    socket: Object,\n    players: Array\n  },\n  data(){\n    return {\n      strings,\n      colors,\n    }\n  },\n  computed: {\n    availableColors: function(){\n      try {\n        let av = [];\n        let used = [];\n        this.players.forEach(player => {\n          used.push(player.color);\n        })\n        for (let i = 0; i < colors.length; i++) {\n          if (!used.includes(i)) av.push(i);\n        }\n        return av;\n      } catch (e) {\n        return colors.keys();\n      }\n    }\n  },\n  methods: {\n    changeColor(color){\n      this.socket.emit(websocketEvents.CHANGE_COLOR, color);\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n.change-color{\n  display: flex;\n  flex-flow: row;\n  justify-content: center;\n  align-items: center;\n  width: 100%;\n  padding-top: 20px;\n  position: relative;\n\n  p{\n    font-size: 150%;\n    color: white;\n  }\n\n  .colors-wrapper{\n    display: flex;\n    flex-wrap: wrap;\n\n    .color-available{\n      width: 40px;\n      height: auto;\n      margin: 20px;\n      transform: rotate(-45deg);\n    }\n  }\n\n}\n\n</style>"],"sourceRoot":""}]}