      return this.game.settings[this.settingKey];\n    },\n    isAdmin: function(){\n      return this.game.admin === this.game.currentPlayer;\n    }\n  },\n  methods: {\n    increase(){\n      if(this.value<this.max && this.isAdmin) this.socket.emit(this.event, this.value+1);\n    },\n    decrease(){\n      if(this.value>this.min && this.isAdmin) this.socket.emit(this.event, this.value-1);\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n.setter{\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n\n  p{\n    color: white;\n    font-size: 150%;\n    margin-right: 20px;\n  }\n\n  div{\n    display: flex;\n    justify-content: space-evenly;\n    align-items: center;\n\n    span{\n      width: 50px;\n      height: 50px;\n      background-color: transparent;\n      border: 2px solid white;\n      color: white;\n      font-size: 250%;\n      outline: none;\n      text-align: center;\n      user-select: none;\n      margin: 10px;\n    }\n\n    div{\n      display: flex;\n      flex-flow: column;\n      justify-content: space-between;\n      align-items: center;\n      margin: 5%;\n      height: 30%;\n\n      .arrow{\n        width: 10px;\n        height: 10px;\n        border-top: 2px solid white;\n        border-left: 2px solid white;\n        transition: all 0.5s;\n\n        &.down{\n          transform: rotate(-135deg);\n\n          &:hover{\n            transform: translateY(10px) rotate(-135deg) scale(1.5);\n          }\n        }\n\n        &.up{\n          transform: rotate(45deg);\n\n          &:hover{\n            transform: translateY(-10px) rotate(45deg) scale(1.5);\n          }\n        }\n      }\n    }\n  }\n\n}\n\n</style>"]}]}