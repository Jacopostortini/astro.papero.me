 socket: Object\n  },\n  data(){\n    return {\n      strings: strings,\n      ready: false,\n      dimensions: false,\n      width: 0,\n      height: 0,\n      points: \"\"\n    }\n  },\n  computed: {\n    checkDimensions: function() {\n      return this.width > this.height;\n    }\n  },\n  methods: {\n    readyUp(){\n      this.socket.emit(websocketEvents.READY_UP);\n      this.ready = true;\n    }\n  },\n  mounted() {\n    setInterval(()=>{\n      if(this.points.length >= 3) {\n        this.points = \"\";\n      } else {\n        this.points += \".\";\n      }\n    }, 1000);\n\n    this.width = window.innerWidth;\n    this.height = window.innerHeight;\n\n    this.dimensions = this.checkDimensions;\n\n    window.addEventListener(\"resize\", () => {\n      this.width = window.innerWidth;\n      this.height = window.innerHeight;\n      this.dimensions = this.checkDimensions;\n    });\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n.resize__main-panel{\n  width: 100%;\n  height: 100%;\n  display: flex;\n  flex-flow: column;\n  justify-content: center;\n  align-items: center;\n\n  p{\n    margin: 0;\n    font-size: 150%;\n    padding-top: 30px;\n  }\n\n  div{\n    height: 100%;\n    display: flex;\n    flex-flow: column;\n    align-items: center;\n    justify-content: space-evenly;\n    padding: 0 5% 0 5%;\n\n    p{\n      margin: 0;\n      color: white;\n      font-size: 150%;\n    }\n  }\n\n  button{\n    font-size: 150%;\n  }\n}\n</style>"]}]}