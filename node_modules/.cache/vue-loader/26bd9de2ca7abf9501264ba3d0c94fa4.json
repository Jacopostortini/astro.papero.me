game.settings[this.settingKey];\r\n    },\r\n    isAdmin: function(){\r\n      return this.game.admin === this.game.currentPlayer;\r\n    }\r\n  },\r\n  methods: {\r\n    increase(){\r\n      if(this.value<this.max && this.isAdmin) this.socket.emit(this.event, this.value+1);\r\n    },\r\n    decrease(){\r\n      if(this.value>this.min && this.isAdmin) this.socket.emit(this.event, this.value-1);\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n.setter{\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n\r\n  p{\r\n    color: white;\r\n    font-size: 150%;\r\n    margin-right: 20px;\r\n  }\r\n\r\n  div{\r\n    display: flex;\r\n    justify-content: space-evenly;\r\n    align-items: center;\r\n\r\n    span{\r\n      width: 50px;\r\n      height: 50px;\r\n      background-color: transparent;\r\n      border: 2px solid white;\r\n      color: white;\r\n      font-size: 250%;\r\n      outline: none;\r\n      text-align: center;\r\n      user-select: none;\r\n      margin: 10px;\r\n    }\r\n\r\n    div{\r\n      display: flex;\r\n      flex-flow: column;\r\n      justify-content: space-between;\r\n      align-items: center;\r\n      margin: 5%;\r\n      height: 30%;\r\n\r\n      .arrow{\r\n        width: 10px;\r\n        height: 10px;\r\n        border-top: 2px solid white;\r\n        border-left: 2px solid white;\r\n        transition: all 0.5s;\r\n\r\n        &.down{\r\n          transform: rotate(-135deg);\r\n\r\n          &:hover{\r\n            transform: translateY(10px) rotate(-135deg) scale(1.5);\r\n          }\r\n        }\r\n\r\n        &.up{\r\n          transform: rotate(45deg);\r\n\r\n          &:hover{\r\n            transform: translateY(-10px) rotate(45deg) scale(1.5);\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n}\r\n\r\n</style>"]}]}