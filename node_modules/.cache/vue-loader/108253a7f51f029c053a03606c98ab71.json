game\");\n    new Phaser.Game(\n        config(\n            [new GameScene(this.socket, {}), new RankingScene(10)],\n            parent,\n            gameDimensions.width,\n            gameDimensions.height,\n            Phaser.Scale.FIT\n        ));\n\n    this.socket.once(websocketEvents.GAME_MODIFIED, game => {\n      const parent = document.getElementById(\"game\");\n      new Phaser.Game(\n          config(\n              [new GameScene(this.socket, game), new RankingScene(game.settings.pointsToWin)],\n              parent,\n              gameDimensions.width,\n              gameDimensions.height,\n              Phaser.Scale.FIT\n          ));\n    });\n    this.socket.emit(websocketEvents.GAME_MODIFIED);\n  }\n}\n</script>\n\n<style scoped>\n\n#game{\n  width: 100%;\n  height: 100%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n</style>"]}]}