ngth >= 3) {\r\n        this.points = \"\";\r\n      } else {\r\n        this.points += \".\";\r\n      }\r\n    }, 1000);\r\n\r\n    this.width = window.innerWidth;\r\n    this.height = window.innerHeight;\r\n\r\n    if(this.checkDimensions) this.readyUp();\r\n\r\n    window.addEventListener(\"resize\", () => {\r\n      this.width = window.innerWidth;\r\n      this.height = window.innerHeight;\r\n      if(this.checkDimensions) this.readyUp();\r\n    });\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n.resize__main-panel{\r\n  width: 100%;\r\n  height: 100%;\r\n\r\n  div{\r\n    height: 100%;\r\n    display: flex;\r\n    flex-flow: column;\r\n    align-items: center;\r\n    justify-content: space-evenly;\r\n    padding: 0 5% 0 5%;\r\n\r\n    p{\r\n      margin: 0;\r\n      color: white;\r\n      font-size: 150%;\r\n    }\r\n\r\n    button{\r\n      font-size: 150%;\r\n    }\r\n  }\r\n}\r\n</style>"]}]}